//1.一个数组中只有两个数字是出现一次，
//其他所有数字都出现了两次。
//找出这两个数字，编程实现。
#include <stdio.h>
void find_data(int arr[],int size){
	int i = 0,j=0;
	int flag = 0;
	for (; i < size;i++){
		flag = 0;
		for (j = 0; j < size; j++){
			if (arr[j] == arr[i] && i != j)
				flag = 1;
		}
		if (0==flag)
		printf("%d ",arr[i]);
	}
	printf("\n");
}
int main(){
	int arr []= { 1, 2, 4, 3, 1, 5, 2, 3 };
	int size = sizeof arr / sizeof arr[0];
	find_data(arr,size); 
	system("pause");
	return 0;
}
